<VBox prefHeight="728.0" prefWidth="836.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="vue.Vue">
  <children>
    <MenuBar prefHeight="10.0" prefWidth="718.0" VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem fx:id="exitMenuItem" mnemonicParsing="false" text="Exit" />
          </items>
        </Menu>
            <Menu mnemonicParsing="false" text="Edit">
               <items>
                  <MenuItem fx:id="dutchStationMenuItem" mnemonicParsing="false" text="Stations en nÃ©erlandais" />
               </items>
            </Menu>
      </menus>
    </MenuBar>
      <VBox fx:id="root" alignment="CENTER" prefHeight="374.0" prefWidth="640.0" spacing="10.0">
         <children>
            <ImageView fitHeight="311.0" fitWidth="732.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@stib-metro-plan.png" />
               </image>
            </ImageView>
            <HBox prefHeight="217.0" prefWidth="642.0" spacing="15.0">
               <children>
                  <VBox alignment="CENTER" prefHeight="314.0" prefWidth="380.0" spacing="15.0">
                     <children>
                        <ImageView fitHeight="81.0" fitWidth="111.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@logo.png" />
                           </image>
                        </ImageView>
                        <Label alignment="CENTER" prefHeight="18.0" prefWidth="114.0" text="Origin:" underline="true" />

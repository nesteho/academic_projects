                                    <Insets bottom="20.0" right="50.0" top="20.0" />
                                 </HBox.margin>
                              </SearchableComboBox>
                              <VBox alignment="CENTER" prefHeight="52.0" prefWidth="247.0">
                                 <children>
                                    <Button fx:id="showPathButton" alignment="CENTER" mnemonicParsing="false" prefHeight="30.0" prefWidth="198.0" text="Show Route">
                                       <VBox.margin>
                                          <Insets bottom="10.0" />
                                       </VBox.margin>
                                    </Button>
                                    <Button fx:id="deleteFavoriteButton" alignment="CENTER" mnemonicParsing="false" prefHeight="26.0" prefWidth="196.0" text="Remove from Favorites">
                                       <VBox.margin>
                                          <Insets />
                                       </VBox.margin>
                                    </Button>
                                 </children>
                              </VBox>
                           </children>
                           <opaqueInsets>
                              <Insets />
                           </opaqueInsets>
                           <padding>
                              <Insets bottom="25.0" top="25.0" />
                           </padding>
                        </HBox>
                     </children>
                  </VBox>
               </children>
            </HBox>
         </children>
         <VBox.margin>

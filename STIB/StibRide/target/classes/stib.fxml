                        <Label text="Destination" underline="true" />
                        <SearchableComboBox fx:id="toSearchBox" promptText="DESTINATION..." />
                        <Button fx:id="searchButton" alignment="CENTER" mnemonicParsing="false" prefHeight="26.0" prefWidth="83.0" text="Search" />
                        <Button fx:id="addFavoriteButton" mnemonicParsing="false" text="Add to Favorites">
                           <VBox.margin>
                              <Insets bottom="10.0" />
                           </VBox.margin>
                        </Button>
                     </children>
                  </VBox>
                  <TableView fx:id="table" prefHeight="324.0" prefWidth="468.0">
                    <columns>
                      <TableColumn fx:id="stationCol" prefWidth="175.99999713897705" text="Stations" />
                      <TableColumn fx:id="linesCol" prefWidth="172.00001525878906" text="Lines" />
                    </columns>
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                     <padding>
                        <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
                     </padding>
                  </TableView>
               </children>
            </HBox>
            <Label fx:id="errorLabel" alignment="TOP_LEFT" contentDisplay="CENTER" prefHeight="73.0" prefWidth="688.0" text="Error : " textAlignment="CENTER" textFill="#de313b" visible="false">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </Label>
            <HBox prefHeight="147.0" prefWidth="806.0">
               <children>
                  <VBox>
                     <children>
                        <Label alignment="CENTER" prefHeight="18.0" prefWidth="114.0" text="Favoris" underline="true">
                           <padding>
                              <Insets top="15.0" />
                           </padding>
                        </Label>
                        <HBox alignment="CENTER" prefHeight="104.0" prefWidth="812.0">
                           <children>
                              <SearchableComboBox fx:id="favoriteBox" prefHeight="26.0" prefWidth="194.0" promptText="FAVORITE...">
